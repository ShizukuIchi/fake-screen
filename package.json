{
  "name": "fake-screen",
  "version": "1.0.0",
  "homepage": "https://sh1zuku.csie.io/fake-screen",
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-report": "yarn test --watch --coverage",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "better-scroll": "^1.13.2",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-pose": "^4.0.5",
    "react-router-dom": "^4.4.0-beta.6",
    "react-scripts": "2.1.3",
    "screenfull": "^3.3.3",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "gh-pages": "^2.0.1",
    "jest-extended": "^0.11.0",
    "react-test-renderer": "^16.7.0",
    "react-testing-library": "^5.4.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/assets/*",
      "!src/components/index.js",
      "!src/themes/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "text"
    ]
  }
}
